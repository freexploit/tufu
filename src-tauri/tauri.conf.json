{
  "build": {
    "beforeDevCommand": "npx elm-land server",
    "beforeBuildCommand": "npx elm-land build",
    "devPath": "http://localhost:1234",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "tufu",
    "version": "0.0.1"
  },
  "tauri": {
    "systemTray": {
      "iconPath": "icons/pomodoro.png",
      "iconAsTemplate": true
    },
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tufu.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "tauri",
        "width": 800,
        "height": 600
      }
    ]
  }
}
